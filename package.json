{
  "name": "react-channel-plugin",
  "version": "1.0.0",
  "description": "Channel Talk plugin wrapper for React",
  "author": "Uk-Jin Jang <doublestat@gmail.com>",
  "repository": "https://github.com/ukjinjang/react-channel-plugin",
  "homepage": "https://channel.io",
  "license": "MIT",
  "keywords": [
    "react",
    "channalio",
    "channal talk",
    "live chat",
    "customer-service"
  ],
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "run-s playground:start",
    "build": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build.es.json",
    "lint": "eslint --ext .ts --ext .tsx ./src",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "playground:start": "snowpack dev --config ./snowpack.config.ts",
    "playground:build": "snowpack build --config ./snowpack.config.ts && del \"build/_*\""
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "tslib": ">=2.2.0"
  },
  "dependencies": {
    "react": "^16.8.0",
    "react-github-btn": "^1.2.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@snowpack/plugin-babel": "^2.1.7",
    "@snowpack/plugin-dotenv": "^2.1.0",
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/plugin-webpack": "^2.3.1",
    "@types/node": "^12.12.62",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.1.9",
    "@types/webpack": "^4.41.6",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-styled-components": "^1.12.0",
    "cypress": "^7.2.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsdoc": "^33.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-app": "^6.2.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-dom": "^16.13.0",
    "snowpack": "^3.3.7",
    "styled-components": "^5.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
