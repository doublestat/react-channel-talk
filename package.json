{
  "name": "react-channel-plugin",
  "version": "1.0.0",
  "description": "Channel Talk plugin wrapper for React",
  "author": "Uk-Jin Jang <doublestat@gmail.com>",
  "repository": "https://github.com/ukjinjang/react-channel-plugin",
  "homepage": "https://channel.io",
  "license": "MIT",
  "keywords": [
    "react",
    "channalio",
    "channal talk",
    "live chat",
    "customer-service"
  ],
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build.es.json",
    "lint": "eslint --ext .ts --ext .tsx ./src",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "storybook": "start-storybook -p 8000 -c .storybook --ci"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "tslib": ">=2.2.0"
  },
  "dependencies": {
    "react": "^16.3.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-backgrounds": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-jest": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@storybook/theming": "^6.2.9",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^12.12.62",
    "@types/react": "^16.0.0",
    "@types/webpack": "^4.41.6",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsdoc": "^33.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-app": "^6.2.2",
    "fs-extra": "^10.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "react-dom": "^16.13.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^4.46.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
